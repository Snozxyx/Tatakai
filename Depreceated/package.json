{
  "name": "webos-tatakai-app",
  "version": "1.0.0",
  "private": true,
  "description": "Tatakai Anime Streaming App for WebOS TVs",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "package": "ares-package dist",
    "install-device": "ares-install -d tv dist",
    "launch": "ares-launch -d tv com.tatakai.webos",
    "inspect": "ares-inspect -d tv com.tatakai.webos",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^10.18.0",
    "hls.js": "^1.4.14",
    "lucide-react": "^0.400.0",
    "react": "^18.2.0",
