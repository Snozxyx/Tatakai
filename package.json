{
  "name": "tatakai",
  "private": true,
  "version": "4.1.0",
  "description": "Tatakai - Watch anime online",
  "author": "Tatakai",
  "license": "MIT",
  "homepage": "https://ggmp.me",
  "main": "desktop/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "npm run build && cross-env NODE_ENV=production vite preview --port 3000",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:8088 && electron desktop/main.cjs\"",
    "electron:build": "vite build && electron-builder",
    "assets:generate": "node scripts/generate-assets.cjs",
    "mobile:dev": "npm run build && npx cap open android",
    "mobile:sync": "npm run build && npx cap sync",
    "mobile:build": "npx cap sync android && cd android && gradlew assembleRelease"
  },
  "dependencies": {
